<h1>Users</h1>
  <table>
    <thead>
      <tr>
        <th>Rating</th>
        <th>Content</th>
      </tr>
    </thead>

    <tbody>
      <% @reviews.each do |review| %>
      <tr>
        <td><%= review.rating %></td>
        <td><%= review.content %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <br>
